<?xml version="1.0"?>

<launch>

	<node name="map_server" pkg="map_server" type="map_server" args="$(find dino_nav)/tracks/maps/lab.yaml"/>

	<node name="scan_matcher" pkg="laser_scan_matcher" type="laser_scan_matcher_node">
		<param name="fixed_frame" value="odom" />
		<param name="base_frame" value="footprint" />
		<param name="use_odom" value="false" />
		<param name="publish_tf" value="true" />
	</node>
	
	<node name="amcl" pkg="amcl" type="amcl">
		<param name="odom_frame_id" value="odom"/>
		<param name="global_frame_id" value="map" />
		<param name="base_frame_id" value="footprint" />

	</node>

</launch>
